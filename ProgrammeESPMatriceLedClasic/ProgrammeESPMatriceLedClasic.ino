void setup() {
  // Initialisation de la matrice
  matrix.begin();

  Serial.begin(115200);

  // Chargement des paramètres enregistrés
  preferences.begin("LEDSettings", false);
  numRepetitions = preferences.getUInt("repetitions", 30);
  delayBetweenAnimations = preferences.getUInt("delay", 20000);
  brightness = preferences.getUInt("brightness", 128);
  frameDuration = preferences.getUInt("frameDuration", 100);
  matrix.setBrightness(brightness);

  // Connexion Wi-Fi
  IPAddress staticIP(192, 168, 1, 28); // Adresse IP statique souhaitée
  IPAddress gateway(192, 168, 1, 1); // Adresse IP de la passerelle (généralement l'adresse IP du routeur)
  IPAddress subnet(255, 255, 255, 0); // Masque de sous-réseau

  WiFi.config(staticIP, gateway, subnet);

  WiFi.begin(ssid, password);
  while (WiFi.status() != WL_CONNECTED) {
    delay(1000);
    Serial.println("Connecting to WiFi...");
  }
  Serial.println("Connected to WiFi");

  // Code du formulaire HTML
  const char* htmlForm =
    R"rawliteral(
  <!DOCTYPE html>
  <html>
  <head>
    <title>ESP32 LED Settings</title>
  </head>
  <body>
    <h1>ESP32 LED Settings</h1>
    <form action="/settings" method="post">
      <label for="repetitions">Number of repetitions:</label>
      <input type="number" id="repetitions" name="repetitions" min="1" required>
      <br>
      <label for="delay">Delay between animations (ms):</label>
      <input type="number" id="delay" name="delay" min="0" required>
      <br>
      <label for="brightness">Brightness (0-255):</label>
      <input type="number" id="brightness" name="brightness" min="0" max="255" required>
      <br>
      <label for="frame_duration">Frame duration (ms):</label>
      <input type="number" id="frame_duration" name="frame_duration" min="10" required>
      <br>
      <input type="submit" value="Submit">
    </form>
  </body>
  </html>
    )rawliteral";

  // Configuration du serveur web
  server.on("/settings", HTTP_POST, [](AsyncWebServerRequest *request) {
    int params = request->params();
    for (int i = 0; i < params; i++) {
      AsyncWebParameter* p = request->getParam(i);
      if (strcmp(p->name().c_str(), "repetitions") == 0) {
        numRepetitions = p->value().toInt();
        preferences.putUInt("repetitions", numRepetitions);
      } else if (strcmp(p->name().c_str(), "delay") == 0) {
        delayBetweenAnimations = p->value().toInt();
        preferences.putUInt("delay", delayBetweenAnimations);
      } else if (strcmp(p->name().c_str(), "brightness") == 0) {
        brightness = p->value().toInt();
        preferences.putUInt("brightness", brightness);
        matrix.setBrightness(brightness);
      } else if (strcmp(p->name().c_str(), "frame_duration") == 0) {
        frameDuration = p->value().toInt();
        preferences.putUInt("frameDuration", frameDuration);
      }
    }
    request->send(200, "text/plain", "Settings updated");
  });

  server.on("/", HTTP_GET, [htmlForm](AsyncWebServerRequest *request) {
    request->send(200, "text/html", htmlForm);
  });

  server.begin();
  Serial.print("ESP32 IP address: ");
  Serial.println(WiFi.localIP());
}


void displayFrame(uint32_t* frame) {
  for (int i = 0; i < LED_COUNT; i++) {
    matrix.setPixelColor(i, frame[i]); // Définition de la couleur du pixel
  }

  matrix.show(); // Affichage de la frame
  delay(frameDuration); // Pause avant d'afficher la frame suivante
}
void displayAnimation(uint32_t* animation, int num_frames) {
  for (int frame_index = 0; frame_index < num_frames; frame_index++) {
    uint32_t* current_frame = animation + frame_index * LED_COUNT; // Sélection de la frame en cours
    displayFrame(current_frame); // Affichage de la frame en cours
  }
}

void loop() {
  while (true) {
    for (int anim_index = 0; anim_index < NUM_ANIMATIONS; anim_index++) {
      for (int i = 0; i < numRepetitions; i++) {
        displayAnimation(animations[anim_index], NUM_FRAMES[anim_index]);
      }

      matrix.fill(BLACK);
      matrix.show();
      delay(delayBetweenAnimations);
    }
  }
}